{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
"logo":{
	"source": "~/.config/fastfetch/Floppy.png"
},
"display": {
	"separator": "   ",
"bar": {
	"charElapsed": "▬",
	"charTotal": "ㆍ",
	"width": 15
    },
"percent": {
	"type": 2
    }
 },
  "modules": [
    "title",
        {
	"format": "─────────────────────────",
	"type": "custom"
    },
        {
      "type": "os",
      "key": " OS",
      "keyColor": "magenta"
    },
       {
	"type": "kernel",
	"key": " Kernel",
	"keyColor": "magenta"
    },
       {
	"type": "packages",
	"key": "󰏖 Packages",
	"keyColor": "magenta"
    },
       {
	"type": "shell",
	"key": " Shell",
	"keyColor": "magenta"
    },
       {
	"type": "uptime",
	"key": "󰅐 Uptime",
	"keyColor": "magenta"
    },
       {
	"format": "─────────────────────────────",
	"type": "custom"
    },
       {
	"type": "wm",
	"key": "󰨇 WM",
	"keyColor": "blue"
    },
       {
	"type": "theme",
	"key": " Theme",
	"keyColor": "blue"
    },
       {
	"type": "icons",
	"key": " Icons",
	"keyColor": "blue"
    },
       {
	"type": "font",
	"key": " Font",
	"keyColor": "blue"
    },
       {
	"type": "terminal",
	"key": " Terminal",
	"keyColor": "blue"
    },
       {
	"type": "terminalfont",
	"key": "󰧴 Terminal Font",
	"keyColor": "blue"
    },
       {
	"format": "────────────────────────────────────────────────",
	"type": "custom"
    },
       {
	"type": "host",
	"key": " Motherboard",
	"keyColor": "red"
    },
	{
	    "type": "cpu",
	    "key": " CPU",
	    "showPeCoreCount": true,
	    "temp": false,
	    "keyColor": "red"
    },
	{
	"type": "gpu",
	"key": "󰾲 GPU",
	"format": "{1} {2}",
	"keyColor": "red"
    },
        {
	"type": "memory",
	"key": " RAM",
	"format": "{4}",
	"keyColor": "red"
    },
        {
	"type": "swap",
	"key": "󰾵 Swap",
	"format": "{4}",
	"keyColor": "red"
    },
        {
	    "type": "disk",
	    "key": "󰋊 Disk",
	    "folders": "/:/mnt/Internal_drives/Data:/mnt/Internal_drives/Data2:/mnt/Internal_drives/Data3",
	    "format": "{3} {13}",
	    "keyColor": "red"
    },
        {
	"format": "────────────────────────────────────────────",
	"type": "custom"
    },
	{
	    "type": "colors",
	    "symbol": "circle"
    }
  ]
}
